<div id="main">
	<h1>Welcome to Rails App</h1>

<% if user_signed_in? %>

	<%= current_user.email %>
	<br>
	
	<%= link_to 'userpage', 'welcome/userpage' %>
	<br>
	<%= link_to 'logout', destroy_user_session_path , method: :delete %>
	<% else %>
	<%= link_to 'Registration', new_user_registration_path %>

	<%= link_to 'Login', new_user_session_path %>
<% end %>

</div>

